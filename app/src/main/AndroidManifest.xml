<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.dicegamesmix.dicegamesmix">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MaexleWelcomeScreen" />
        <activity android:name=".MaexlePlayerScreen" />
        <activity android:name=".MaexleGameScreen" />
        <activity android:name=".MaexleHighscoreScreen" />
        <activity android:name=".Dice" />
        <activity android:name=".MaexleThrowBeginning" />
        <activity android:name=".MaexleThrowMiddle" />
        <activity android:name=".MaexleAnswerPlayer" />
        <activity android:name=".MaexleAnswerUncover"></activity>
    </application>

</manifest>